	},
	"anim_fade_out": {
		"anim_type": "alpha",
		"easing": "in_quart",
		"from": 1,
		"to": 0
	},
	"anim_alpha_wait": {
		"anim_type": "alpha",
		"easing": "in_quart",
		"from": 0,
		"to": 0
	},
	"anim_appear": {
		"anim_type": "alpha",
		"easing": "linear",
		"from": 1,
		"to": 1
	},
	"anim_off_wait": {
		"anim_type": "offset",
		"easing": "linear",
		"from": [
			"-2000%",
			"-2000%"
		],
		"to": [
			"-2000%",
			"-2000%"
		]
	},
	"anim_off_in": {
		"anim_type": "offset",
		"easing": "linear",
		"from": [
			"(100%-100%cm)/2)%",
			"50%+50%y"
		],
		"to": [
			"(100%-100%cm)/2)%",
			0
		]
	},
	"anim_off_out": {
		"anim_type": "offset",
